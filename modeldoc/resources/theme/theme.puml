@startuml
package Theme {

  class "Theme\nPublication" as tp
  class "Theme" as th
  class "FileType" as ft 
  
  abstract class "DataProduct" as dp << package product >>
  
  package Organisation {
    class "OrgUnit" as ou
    note right
      AGI ist als tech. 
      OrgUnit aller Themes
      hier auch erfasst.
      Link wird f√ºr jede
      ThemePub implizit
      gesetzt.
    end note
    
    class "Agency" as ag
    class "SubOrg" as so
    
    ag "1" -right- "*" so
  
    ou <|-- ag
    ou <|-- so
  }
  
  package "SubArea" {
    class "SubArea" as sa
    class "Published\nSubArea" as pa
    
    pa "*" -- "0..1" sa
  }
  
  tp "1" -- "1..n" pa
  
  th "1" -- "*" tp
  th "*" -- "1" ou : "fach"
  tp "0..1" -- "1-n" dp : (b1)
  tp "*" -- "*" ft
}

legend
  b1: DSV sind immer einer ThemePub zuzuweisen --> SoftwareConstraint
end legend
@endumldc